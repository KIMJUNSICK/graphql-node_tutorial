type Link {
  id: ID! @id
  description: String!
  url: String!
  postBy: User
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type User {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
  links: [Link!]!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}
